    <id>http://www.sistrall.it/photographs/2018/03/20/al-fondo-della-cascata/</id>
    <published>2018-03-20T14:51:08+00:00</published>
    <updated>2018-03-22T12:58:40+00:00</updated>
    <author>
      <name>Silvano Stralla</name>
    </author>
